<section class="section text-page" id="@("C" + Objref.Id)">
	@if (!string.IsNullOrEmpty(Objref.PluginContent.Title))
	{
		<h1 class="text-page__title">@Objref.PluginContent.Title</h1>
	}
	@if (!string.IsNullOrEmpty(Objref.PluginContent.Perex))
	{
		<p class="text-page__subtitle">@Objref.PluginContent.Perex</p>
	}
	@if (!string.IsNullOrEmpty(Objref.PluginContent.Note))
	{
		<p class="text-page__subtitle">@((MarkupString)Objref.PluginContent.Note)</p>
	}
</section>


@code {
	[Parameter]
	public webManagerCMS.Core.PageContentNS.Plugins.DocH1Text? Objref { get; set; }

}
